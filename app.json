{
    "expo": {
        "name": "Tindquette",
        "slug": "Tindquette",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/logo.png",
        "splash": {
            "image": "./assets/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "ios.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/logo.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "android.app"
        },
        "web": {
            "favicon": "./assets/logo.png"
        }
    }
}
